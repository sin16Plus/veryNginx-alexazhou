  "admin":[{
      "user":"verynginx",
      "password":"verynginx",
      "enable":true
    }],
  "dashboard_host":"",
  "response":{
    "hit":{
      "content_type":"text/html",
      "body":"hited"
    },
    "didn'thit":{
      "content_type":"text/html",
      "body":"didn't hited"
    }
  },
  "scheme_lock_enable":true,
  "uri_rewrite_rule":[],
