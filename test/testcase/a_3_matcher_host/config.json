      "action":"block"
    },{
      "enable":true,
      "code":"400",
      "response":"hit",
      "matcher":"host_not_match",
      "custom_response":true,
      "action":"block"
    }],
  "config_version":"0.36",
  "static_file_rule":[],
  "proxy_pass_rule":[],
  "summary_collect_rule":[],
